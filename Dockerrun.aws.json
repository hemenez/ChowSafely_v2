{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "web",
            "image": "lhemenez/web",
            "hostname": "web",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "lhemenez/nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["web"]
        }
    ]
}